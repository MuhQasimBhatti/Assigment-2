// Fibocanci series
// 0,1,1,2,3,5,8,13
// ..... sum=a+b
#include<iostream>
using namespace std;
int main()
{
	int a = 0;
	int d = 0;
	int b = 1;
	int sum = 0;
	int x;
	double long* arr = new double long[d];
	cout << "Enter a number ";
	cin >> x;

	while (a < x)
	{
		cout << a << ",";
		sum = a + b;
		a = b;
		b = sum;
		d = d + 1;
	}
	cout << endl<< d;

	for (int z = 0; z < d; z++)
		cin >> arr[z];

	for (int z = 0; z < d; z++)
		cout << arr[z];

}
