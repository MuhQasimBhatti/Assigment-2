
#include <iostream>
#include <cmath>
using namespace std;

int main()
{
	int a;
	int b;
	int i = 1;
	int c = 0;
	int p = 10;
	cout << "Enter a number upto 6 digits" << endl;
		cin >> a;
		if (a <= 999999)
		{
			b = a;
			while (b > 0)
			{
				b = b / 10;
				c++;
			}
			p = pow(p, (c - 1));
			for (int i = 1; i <= c; i++)
			{
				for (int j = 1; j < 2; j++)
					cout << a / p;
				p = p / 10;
				cout << endl;
			}
		}
}
