#include<iostream>
using namespace std;
int main() {
	int y=0;
	int x=0;
	int a=0;
	int min = 0;
	int Entries = 0;
	long * arr = new long[Entries];

	cout << "enter numbers" << endl;
	while (x < 9999)
	{
		cin >> x;
		arr[Entries] = x;
		Entries++;
	}
	cout << Entries << endl;
	
	for (int i = 0; i <= Entries; i++)
		cout << arr[i]<<endl;

	for (int z = 0; z < Entries; z++)
		for (int i = z + 1; i < Entries; i++)
			if (arr[z] < arr[i])
			{
				 y = arr[z];
				
			}
	cout << y;
}
