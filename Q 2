#include<iostream>
using namespace std;
int main() {
	int x = 0;
	int min = 0;
	int Entries = 0;
	long* arr = new long[Entries];

	cout << "enter numbers" << endl;
	while (x < 9999)
	{
		cin >> x;
		arr[Entries] = x;
		Entries++;
	}
	min = arr[0];
	for (int y = 0; y < Entries; y++)
			if (min > arr[y])
				min = arr[y];
	cout <<"The smallest number is" << min ;

}
