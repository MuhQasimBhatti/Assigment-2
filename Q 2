#include<iostream>
using namespace std;
int main() {
	int y = 0;
	int x = 0;
	int a = 0;
	int min = 0;
	int Entries = 0;
	long* arr = new long[Entries];

	cout << "enter numbers" << endl;
	while (x < 9999)
	{
		cin >> x;
		arr[Entries] = x;
		Entries++;
	}
	
	y = arr[0];
	for (int i = 0; i < Entries; i++)
		if (y > arr[Entries])
			y = arr[Entries];
	cout << y;

}
