/* A program that records n integer from user and print
1	1 2 3 4 5
2	2 3 4 5 1
3	3 4 5 1 2
4	4 5 1 2 3
5	5 4 3 2 1

	1,1 1,2 1,3 1,4	1,5
	2,1 2,2 2,3 2,4 2,5
	3,1 3,2 3,3 3,4 3,5
	4,1 4,2 4,3 4,4 4,5
	5,1 5,2 5,3 5,4 5,5
	*/
#include<iostream>
using namespace std;
int main()
{
	int n=1;
	int x;
	cin >> x;
	for (int i = 1; i <= x; i++)
	{
		for (int j = i; j <= x; j++)
			cout << j << " ";
		for (int k = 1; k < i; k++)
			cout << k<<" ";
		
	cout << endl;
	}
	cout << endl << endl << endl;

	
	cin >> x;
	for (int i = 1; i <= x; i++)
	{
		for (int j = i; j <= x; j++)
			cout << j << " ";
		for (int k = 1; k < i; k++)
			cout << k << " ";
		cout << endl;
	}

}
